                                    <div class="card text-white bg-success">
                                        <div class="card-body p-2">
                                            <h5 class="card-title">Credits</h5>
                                            <p class="card-text">
                                                {{ range $key, $value := .App.AlbumMetaData.MetaData.OtherCredits }}
                                                    {{ if ne $value "" }}
                                                        {{$value}}
                                                    {{ end }}
                                                {{ end }}
                                            </p>
                                        </div>
                                    </div>

                                </div>
                            {{ end }}

