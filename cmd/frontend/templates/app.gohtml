                                    <div class="card text-white bg-success">
                                        <div class="card-body p-2">
                                            <h5 class="card-title">Label</h5>
                                            <p class="card-text">
                                                {{ range $key, $value := .App.AlbumMetaData.MetaData.Label }}
                                                    {{ if ne $value "" }}
                                                        {{$value}}
                                                    {{ end }}
                                                {{ end }}
                                            </p>
                                        </div>
                                    </div>

                                </div>
                            {{ end }}

                            {{ if .App.AlbumMetaData.HasCredits }}
