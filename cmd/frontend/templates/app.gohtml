                        {{ end }}

                        {{ if not .IsLoggedIn }}
                            <div class="alert alert-primary" role="alert"><a href="/login">Login</a> to highlight your achievements for this game.</div>
                        {{ end }}

                        <div class="row">
                            <div class="col-6 col-md-4 col-lg-3 mb-3">
                                <div role="button" class="btn btn-success btn-block mb-0 no-cursor-pointer">
                                    Achievements<br/><strong>{{ comma .App.AchievementsCount }}</strong>
                                </div>
                            </div>

                            <div class="col-6 col-md-4 col-lg-3 mb-3">
                                <div role="button" class="btn btn-success btn-block mb-0 no-cursor-pointer">
                                    Average Completion<br/><strong>{{ commaf .App.AchievementsAverageCompletion }}%</strong>
                                </div>
                            </div>
                        </div>

                        <div class="card mb-4">
                            <div class="card-header">Achievement Distribution</div>
                            <div class="card-body">
                                <div id="achievement-counts-chart">
                                    <i class="fas fa-spinner fa-spin"></i>
                                </div>
                            </div>
                            <small class="card-footer">Data from Global Steam players who have at least one achievement</small>
                        </div>

